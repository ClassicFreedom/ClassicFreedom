{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_ADMIN_USERNAME": "@admin_username",
    "NEXT_PUBLIC_ADMIN_PASSWORD": "@admin_password",
    "MONGODB_URI": "@mongodb_uri"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 